ggplot(sexvalue,aes(x = "", y = DayUsedTime, fill = 성별)) + geom_bar(width = 1, stat = "identity") +
coord_polar("y") + geom_text(aes(label = paste0(round(DayUsedTime,10),"시간")),position = position_stack(vjust = 0.5)) + theme_void() + title("서울시 1일 평균 인터넷 사용시간(성별)", family = "Sunflower", cex.main = 2)+title(ylab = "Frequency", family = "Sunflower", cex.lab = 2) + text(2, 70, "N = 1000", family = "Sunflower", cex = 2.5) + theme(plot.title = element_text(hjust = 0.6))
